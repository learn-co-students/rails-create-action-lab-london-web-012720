<!-- Create your form here -->
<h3>Student Form</h3>

<%= form_tag students_path do %>

  <h1><label>First Name:</label><br>
  <%= text_field_tag :first_name %><br></h1>

  <h1><label>Last Name:</label><br>
  <%= text_field_tag :last_name %><br></h1>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= submit_tag "Submit Student" %>

<% end %>